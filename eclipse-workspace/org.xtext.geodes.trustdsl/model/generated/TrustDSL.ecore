<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="trustDSL" nsURI="http://www.xtext.org/geodes/trustdsl/TrustDSL"
    nsPrefix="trustDSL">
  <eClassifiers xsi:type="ecore:EClass" name="TrustModel">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//Element" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Element">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Agent" eSuperTypes="#//Element"/>
  <eClassifiers xsi:type="ecore:EClass" name="UncertainElement" eSuperTypes="#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="beliefs" upperBound="-1"
        eType="#//Belief" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Belief" eSuperTypes="#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="agent" eType="#//Agent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="opinion" eType="#//SBoolean"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SBoolean">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="belief" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="disbelief" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="uncertainty" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="baseRate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UncertainElementFusion" eSuperTypes="#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="beliefs" upperBound="-1"
        eType="#//Belief" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="aleatoryCumulativeBF" eType="#//SBoolean"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="averageingBF" eType="#//SBoolean"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="beliefConstraintFusion"
        eType="#//SBoolean" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="consensusCompromiseBF"
        eType="#//SBoolean" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="epistemicCumulativeBF"
        eType="#//SBoolean" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="weightedBF" eType="#//SBoolean"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
