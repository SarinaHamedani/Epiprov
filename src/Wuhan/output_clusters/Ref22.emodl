(import (rnrs) (emodl cmslib))

(start-model "Ref22")

(param hosp_stay_postICU 21)  ; Average length of stay in hospital after ICU (days) [ref 22]
(param icu_stay 21)  ; Average length of stay in ICU (days) [ref 22]
(param kappa 0.26)  ; Mortality rate for ICU cases [ref 22]
(reaction death_icu (ICU) (D) (* kappa ICU))
(reaction recovery_icu (ICU) (H_postICU) (* (- 1 kappa) (* gamma_severe ICU)))


(end-model)